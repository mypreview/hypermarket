{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"duotone": [
				{
					"colors": [ "#77cde3", "#f5f5f5" ],
					"slug": "primary-and-tile",
					"name": "Primary and tile"
				},
				{
					"colors": [ "#ef0568", "#f5f5f5" ],
					"slug": "danger-and-tile",
					"name": "Danger and tile"
				},
				{
					"colors": [ "#a7c04d", "#f5f5f5" ],
					"slug": "success-and-tile",
					"name": "Success and tile"
				},
				{
					"colors": [ "#606060", "#f5f5f5" ],
					"slug": "entry-and-tile",
					"name": "entry and tile"
				}
			],
			"gradients": [
				{
					"slug": "gray",
					"gradient": "linear-gradient(to right, rgb(238,238,238) 0%, rgb(169,184,195) 100%)",
					"name": "Gray"
				},
				{
					"slug": "yellow-to-red",
					"gradient": "linear-gradient(to right, rgb(254,205,165) 0%, rgb(254,45,45) 50%, rgb(107,0,62) 100%)",
					"name": "Yellow to red"
				},
				{
					"slug": "yellow-to-purple",
					"gradient": "linear-gradient(to right, rgb(255,203,112) 0%, rgb(199,81,192) 50%, rgb(65,88,208) 100%)",
					"name": "Yellow to purple"
				},
				{
					"slug": "barley-white-to-blue",
					"gradient": "linear-gradient(to right, rgb(255,245,203) 0%, rgb(182,227,212) 50%, rgb(51,167,181) 100%)",
					"name": "Barley white to blue"
				},
				{
					"slug": "green",
					"gradient": "linear-gradient(to right, rgb(202,248,128) 0%, rgb(113,206,126) 100%)",
					"name": "Green"
				},
				{
					"slug": "blue",
					"gradient": "linear-gradient(to right, rgb(2,3,129) 0%, rgb(40,116,252) 100%)",
					"name": "Blue"
				}
			],
			"link": true,
			"palette": [
				{
					"slug": "quote",
					"color": "#333333",
					"name": "Quote"
				},
				{
					"slug": "entry",
					"color": "#606060",
					"name": "Entry"
				},
				{
					"slug": "entry-alt",
					"color": "#999999",
					"name": "Entry alternate"
				},
				{
					"slug": "border",
					"color": "#ededed",
					"name": "Border"
				},
				{
					"slug": "tile",
					"color": "#f5f5f5",
					"name": "Tile"
				},
				{
					"slug": "tile-alt",
					"color": "#f0f0f0",
					"name": "Tile alternate"
				},
				{
					"slug": "primary",
					"color": "#77cde3",
					"name": "Primary"
				},
				{
					"slug": "primary-alt",
					"color": "#51bfdb",
					"name": "Primary alternate"
				},
				{
					"slug": "info",
					"color": "#93c4ef",
					"name": "Info"
				},
				{
					"slug": "success",
					"color": "#a7c04d",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#ffce2b",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "#ef0568",
					"name": "Danger"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				}
			]
		},
		"layout": {
			"contentSize": "1170px",
			"wideSize": "1920px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
                "operator": "+",
                "increment": 10,
                "steps": 5,
                "mediumStep": 30,
                "unit": "px"
            },
            "spacingSizes": [
            	{
                    "name": "Smaller",
                    "size": "10px",
                    "slug": "10"
                },
                {
                    "name": "Small",
                    "size": "20px",
                    "slug": "20"
                },
                {
                    "name": "Medium",
                    "size": "30px",
                    "slug": "30"
                },
                {
                    "name": "Large",
                    "size": "40px",
                    "slug": "40"
                },
                {
                    "name": "Larger",
                    "size": "50px",
                    "slug": "50"
                }
            ],
			"units": [ "%", "em", "px", "rem", "vh", "vw" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Work Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [ "file:./fonts/WorkSans-Light.woff2" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [ "file:./fonts/WorkSans-Regular.woff2" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [ "file:./fonts/WorkSans-Medium.woff2" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [ "file:./fonts/WorkSans-SemiBold.woff2" ]
						}
					],
					"fontFamily": "\"Work Sans\", ui-sans",
					"name": "Work Sans",
					"slug": "work-sans"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "ui-monospace, Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"name": "System Monospace",
					"slug": "system-monospace"
				}
			],
			"fontSizes": [
				{	
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "16px",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "23px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "26px",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "37px",
					"slug": "huge"
				}
			],
			"lineHeight": true
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--entry)"
                        }
                    }
                }
			},
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"right": "22px",
						"bottom": "10px",
						"left": "22px"
					}
				}
			},
			"core/calendar": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--entry)"
                        }
                    }
                }
			},
			"core/categories": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--entry)"
                        }
                    }
                }
			},
			"core/navigation": {
				"elements": {
                    "link": {
                        ":hover": {
							"color": {
								"text": "#a6a6a6"
							}
						}
					}
                },
                "spacing": {
                    "blockGap": "30px"
                },
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase"
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"top": "24px",
						"right": 0,
						"bottom": "24px",
						"left": 0
					},
					"margin": {
						"bottom": "24px"
					}
				},
				"typography": {
					"lineHeight": "1.5",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/rss": {
				"spacing": {
					"padding": {
						"left": 0
					},
					"margin": {
						"bottom": 0
					}
				}
			},
			"core/site-title": {
				"color": "var(--wp--preset--color--entry)",
				"typography": {
					"fontSize": "calc(var(--wp--preset--font-size--normal) * 1.25)",
					"fontWeight": "500",
					"lineHeight": "1.5",
					"textTransform": "uppercase"
				},
				"elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--entry)"
                        },
                        "typography": {
							"textDecoration": "none"
						}
                    }
                }
			},
			"core/tag-cloud": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--entry)"
                        }
                    }
                }
			},
			"woocommerce/mini-cart": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--entry)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "300",
					"lineHeight": "1.3",
					"fontSize": "calc(var(--wp--preset--font-size--normal) * 2.5)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "30px"
					}
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "300",
					"lineHeight": "1.3",
					"fontSize": "calc(var(--wp--preset--font-size--normal) * 1.875)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "24px"
					}
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "300",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "28px"
					}
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "1.3",
					"fontSize": "calc(var(--wp--preset--font-size--normal) * 1.375)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "34px"
					}
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "1.3",
					"fontSize": "calc(var(--wp--preset--font-size--normal) * 1.125)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "48px"
					}
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "500",
					"lineHeight": "1.3",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"spacing": {
					"margin": {
						"top": 0,
						"bottom": "48px"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "24px",
			"margin":{
                "top": 0,
                "right": 0,
                "bottom": 0,
                "left": 0
            }
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--work-sans)",
			"lineHeight": "1.5",
			"fontSize": "var(--wp--preset--font-size--normal)"
		}
	}
}